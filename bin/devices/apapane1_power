import os
import sys
import time

home = os.getenv('HOME')
sys.path.append(home+'/src/lib/python/')
from nps import NPS

# SPECIFIC PARAMETERS FOR THIS DEVICE

npsname = "apapane1_power"
npsid = 3
portid = 3

description = "power switch for Apapane C-RED ONE"

# GENERIC CODE

args = sys.argv[1:]
NPS(npsname, npsid, portid, args=args, description=description)

