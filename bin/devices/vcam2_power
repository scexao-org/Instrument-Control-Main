import os
import sys
import time

home = os.getenv('HOME')
sys.path.append(home+'/src/lib/python/')
from nps import NewNPS

# SPECIFIC PARAMETERS FOR THIS DEVICE

npsname = "vcam2_power"
npsid = 2
portid = 16

description = "power switch for VCAM2"

# GENERIC CODE

args = sys.argv[1:]
NewNPS(npsname, npsid, portid, args=args, description=description)

