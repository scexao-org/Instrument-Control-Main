import os
import sys
import time

home = os.getenv('HOME')
sys.path.append(home+'/src/lib/python/')
from nps import nps

# SPECIFIC PARAMETERS FOR THIS DEVICE

npsname = "ocam_power"
npsid = 2
portid = 4

description = "power switch for the OCAM camera"

# GENERIC CODE

args = sys.argv[1:]
nps(npsname, npsid, portid, args=args, description=description)

