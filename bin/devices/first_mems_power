import os
import sys
import time

home = os.getenv('HOME')
sys.path.append(home+'/src/lib/python/')
from nps import NPS

# SPECIFIC PARAMETERS FOR THIS DEVICE

npsname = "first_mems_power"
npsid = 2
portid = 1

description = "power switch for the FIRST MEMS"

# GENERIC CODE

args = sys.argv[1:]
NPS(npsname, npsid, portid, args=args, description=description)

