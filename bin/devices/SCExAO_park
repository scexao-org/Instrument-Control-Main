#!/bin/bash

piaa1 wheel home
piaa2 wheel home
pupil wheel home
smi d home
fpm wheel home
lyot wheel home
starbowl goto 4
fwheel 4
pywfs_pickoff goto 7
pywfs wheel 1
superK irnd 3
superK optnd 3
superK filter 1
 
#invpiaa x goto 4200000

sleep 60

echo " PIAA1"
piaa1 wheel status

echo "       "
echo " PIAA2"
piaa2 wheel status

echo "       "
echo " SMI d "
smi d status

echo "       "
echo " FPM "
fpm wheel status

echo "       "
echo " Lyot "
lyot wheel status

echo "       "
echo " Starbowl "
starbowl status

echo "       "
echo " Fwheel "
fwheel status

echo "       "
echo " Pywfs_pickoff "
pywfs_pickoff status

echo "       "
echo " PyWFS_wheel "
pywfs wheel status

echo "       "
echo " SuperK Irnd "
superK irnd status

echo "       "
echo " SuperK Optnd "
superK optnd status

echo "       "
echo " SuperK filter "
superK filter status

echo "       "
echo " Inverse PIAA"
invpiaa x status

