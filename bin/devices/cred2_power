import os
import sys
import time

home = os.getenv('HOME')
sys.path.append(home+'/src/lib/python/')
from nps import nps

# SPECIFIC PARAMETERS FOR THIS DEVICE

npsname = "cred2_power"
npsid = 3
portid = 6

description = "power switch for the C-RED2 cameras"

# GENERIC CODE

args = sys.argv[1:]
nps(npsname, npsid, portid, args=args, description=description)

